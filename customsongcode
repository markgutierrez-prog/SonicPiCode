  sample :bd_fat
  sleep 1
  sample :bd_fat
  sleep 0.4
  sample :bd_fat
  sleep 0.4
  sample :bd_fat
  sleep 0.5
  sample :bd_fat
  sleep 0.2
  sample :bd_fat
  sleep 1
end
sleep cliplength / 3
live_loop :layerc do
  sample :loop_amen, rate: 0.7
  sleep sample_duration(:loop_amen, rate: 0.7)
end
