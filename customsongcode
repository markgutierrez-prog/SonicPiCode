use_bpm 60
clip = "C:/Users/mg730/Documents/Audacity/DeathByGlamour.mp3"
cliplength = sample_duration "C:/Users/mg730/Documents/Audacity/DeathByGlamour.mp3"
live_loop :clipped do
  sample clip
  sleep cliplength
end
sleep cliplength / 3
live_loop :layera do
  sample :sn_dolf, amp: 0.5
  sleep 0.5
  sample :sn_dolf, amp: 0.5
  sleep 0.5
  sample :sn_dolf, amp: 0.5
  sleep 0.5
end
sleep cliplength / 3
